<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <context-param>
        <param-name>dbname</param-name>
        <param-value>USERS</param-value>
    </context-param>
    <servlet>
        <servlet-name>UserServLet</servlet-name>
        <servlet-class>com.UserServLet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewUser</servlet-name>
        <servlet-class>pages.NewUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update</servlet-name>
        <servlet-class>pages.Update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>pages.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>requestBooking</servlet-name>
        <servlet-class>pages.requestBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AssignDriver</servlet-name>
        <servlet-class>pages.AssignDriver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InvoiceViewer</servlet-name>
        <servlet-class>pages.InvoiceViewer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetPrice</servlet-name>
        <servlet-class>pages.SetPrice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reporting</servlet-name>
        <servlet-class>pages.Reporting</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateMapURL</servlet-name>
        <servlet-class>pages.CreateMapURL</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServLet</servlet-name>
        <url-pattern>/UserService.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewUser</servlet-name>
        <url-pattern>/NewUser.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update</servlet-name>
        <url-pattern>/Update.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutService.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>requestBooking</servlet-name>
        <url-pattern>/requestBooking.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AssignDriver</servlet-name>
        <url-pattern>/AssignDriver.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InvoiceViewer</servlet-name>
        <url-pattern>/InvoiceViewer.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetPrice</servlet-name>
        <url-pattern>/SetPrice.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reporting</servlet-name>
        <url-pattern>/Reporting.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateMapURL</servlet-name>
        <url-pattern>/CreateMapURL.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            20
        </session-timeout>
    </session-config>
</web-app>
